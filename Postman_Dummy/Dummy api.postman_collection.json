		{
			"name": "Create user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"let requestData = pm.request.body.raw ? JSON.parse(pm.request.body.raw) : null;\r",
							"let urlencodedData = pm.request.body.urlencoded;\r",
							"\r",
							"let first_name = urlencodedData.find(item => item.key === \"firstName\").value;\r",
							"\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"id\");\r",
							"});\r",
							"\r",
							"pm.environment.set(\"email\", jsonData.email);\r",
							"pm.environment.set(\"userid\", jsonData.id);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code name has string=ок\", function () {\r",
							"    pm.expect(pm.response.reason()).to.eql(\"OK\");\r",
							"});\r",
							"\r",
							"pm.test(\"First name from request=first name from response\", function () {\r",
							"    pm.expect(jsonData.firstName, \"firstName in response\").to.exist;\r",
							"    pm.expect(first_name, \"firstName in request\").to.exist;\r",
							"    pm.expect(jsonData.firstName).to.eql(first_name);\r",
							"    console.log(\"First name from request:\", first_name);\r",
							"    console.log(\"First name from response:\", jsonData.firstName);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
